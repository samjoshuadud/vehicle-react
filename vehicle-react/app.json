{
  "expo": {
    "name": "vehicle",
    "slug": "vehicle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "vehicle",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "samjoshuadud",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.samjoshuadud.vehicle"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.samjoshuadud.vehicle"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e0e5b2b9-9be7-4b2a-b02d-3b48b32fbcc7"
      }
    }
  }
}
